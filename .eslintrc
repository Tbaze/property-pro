{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "import/prefer-default-export": "on",
    "comma-dangle": ["error",{
      "functions": "ignore"
    }]
  }
}